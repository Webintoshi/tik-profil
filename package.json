{
  "name": "tikprofil-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "TikPofil Monorepo - Web & Mobile",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mobile": "pnpm --filter tikprofil-mobile dev",
    "web": "pnpm --filter tikprofil-web dev"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
