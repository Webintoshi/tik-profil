# Environment Variables (Supabase)
# Copy this file to .env.local and fill in your Supabase credentials

# Supabase (server)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ANON_KEY=

# Supabase (client)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Security
SESSION_SECRET=

# Optional: Supabase schema apply helpers
SUPABASE_DB_PASSWORD=
SUPABASE_POOLER_HOST=
SUPABASE_POOLER_PORT=6543
SUPABASE_POOLER_USER=
SUPABASE_POOLER_DB=postgres

# Optional: legacy storage scan/migration helpers
LEGACY_STORAGE_DOMAINS=
LEGACY_STORAGE_REGEX=

# Optional: Sentry Error Tracking
# Get your DSN from https://sentry.io/settings/projects/
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_DSN=

# Cloudflare R2 Storage (for file uploads)
# Get these from your Cloudflare dashboard
CLOUDFLARE_R2_ACCOUNT_ID=
CLOUDFLARE_R2_ACCESS_KEY_ID=
CLOUDFLARE_R2_SECRET_ACCESS_KEY=
CLOUDFLARE_R2_BUCKET_NAME=
CLOUDFLARE_R2_PUBLIC_URL=
